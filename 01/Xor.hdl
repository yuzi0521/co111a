/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=x);
    Not(in=b,out=y);
    Or(a=a,b=y,out=z);
    Or(a=x,b=b,out=r);
    Nand(a=z,b=r,out=out);
}
